<?xml version="1.0" encoding="UTF-8"?>
<com.mtcflow.model:MTC xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:com.mtcflow.model="http://com.mtcflow.mtc/1.0">
  <metamodels name="MVC" URI="http://co.edu.uniandes.csw.mvc/1.0" alias="MVC" autoregister="false"/>
  <metamodels name="UML" URI="http://www.eclipse.org/uml2/4.0.0/UML" alias="UMl" autoregister="false"/>
  <models name="MVC" file="${outputModel}" metamodel="//@metamodels.0"/>
  <models name="Entities UML " file="model.uml" metamodel="//@metamodels.1"/>
  <models name="Use  Case UML" file="model.uml" metamodel="//@metamodels.1"/>
  <inputModels model="//@models.1" alias="UMLCLASS" transformation="//@transformations.0"/>
  <inputModels model="//@models.2" alias="UMLUCASE" transformation="//@transformations.0"/>
  <transformations xsi:type="com.mtcflow.model:M2MTransformation" name="UML -> MVC" file="uml2mvc.etl"/>
  <outputModels alias="MVC" model="//@models.0" transformation="//@transformations.0"/>
  <configurations>
    <variables name="outputModel" defaultValue="genFromUML/MVC.xmi"/>
  </configurations>
</com.mtcflow.model:MTC>
